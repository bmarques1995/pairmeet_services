{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "service debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Linux/Debug/bin/SampleServer",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
                }
            ]
        }
    ]
}